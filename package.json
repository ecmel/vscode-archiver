{
  "name": "vscode-archiver",
  "displayName": "Archiver",
  "description": "Workspace folder archiver",
  "version": "0.0.21",
  "publisher": "ecmel",
  "license": "MIT",
  "homepage": "https://github.com/ecmel/vscode-archiver",
  "bugs": "https://github.com/ecmel/vscode-archiver/issues",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ecmel/vscode-archiver.git"
  },
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "archive",
    "backup",
    "zip",
    "workspace",
    "folder"
  ],
  "activationEvents": [
    "onCommand:extension.archive"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.archive",
        "title": "Archive Folder"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "archiver": "1.1.0",
    "parse-gitignore": "0.3.1"
  }
}