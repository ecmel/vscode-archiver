{
    "name": "vscode-archiver",
    "displayName": "Archiver",
    "description": "Workspace folder archiver",
    "version": "0.0.8",
    "publisher": "ecmel",
    "engines": {
        "vscode": "^1.0.0"
    },
    "homepage": "https://github.com/ecmel/vscode-archiver",
    "bugs": "https://github.com/ecmel/vscode-archiver/issues",
    "license": "MIT",
    "categories": [
        "Other"
    ],
    "keywords": [
        "archive",
        "backup",
        "zip",
        "workspace",
        "folder"
    ],
    "preview": true,
    "icon": "icon.png",
    "activationEvents": [
        "onCommand:extension.archive"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.archive",
                "title": "Archive Folder"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    },
    "dependencies": {
        "archiver": "1.0.0",
        "parse-gitignore": "0.2.0"
    }
}