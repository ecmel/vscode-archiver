{
    "name": "vscode-archiver",
    "displayName": "vscode-archiver",
    "description": "Make Easy Backups",
    "version": "0.0.2",
    "publisher": "ecmel",
    "engines": {
        "vscode": "^1.0.0"
    },
    "homepage": "https://github.com/ecmel/vscode-archiver",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.archive"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.archive",
                "title": "Archive Folder"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^0.11.0"
    },
    "dependencies": {
        "archiver": "1.0.0",
        "parse-gitignore": "0.2.0"
    }
}